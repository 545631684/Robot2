.pages{  background: linear-gradient(top , rgb(27, 56, 76) , rgb(23, 41, 58) 50% , rgb(27, 35, 49) 100%);
background: -o-linear-gradient(top , rgb(27, 56, 76) , rgb(23, 41, 58) 50% , rgb(27, 35, 49) 100%);
background: -ms-linear-gradient(top , rgb(27, 56, 76) , rgb(23, 41, 58) 50% , rgb(27, 35, 49) 100%);
background: -moz-linear-gradient(top , rgb(27, 56, 76) , rgb(23, 41, 58) 50% , rgb(27, 35, 49) 100%);
background: -webkit-linear-gradient(top , rgb(27, 56, 76) , rgb(23, 41, 58) 50% , rgb(27, 35, 49) 100%); box-sizing: border-box; height: auto; min-height: 100vh; padding-bottom: 150rpx;}
.h20{height: 40rpx;}
.totalInfo{ height: auto; width: 100%; display: flex; justify-content: center;align-items: center; padding: 60rpx 0;}
.totalInfo .totalInfo_l{width: 250rpx; height: 120rpx;  }
.totalInfo .totalInfo_l text{text-align: center; font-weight: bold; display: block; font-family:PingFangSC-Semibold,PingFang SC;}
.totalInfo .totalInfo_l text:nth-child(1){font-size: 40rpx; color: #5d91ff; padding-bottom: 20rpx;}
.totalInfo .totalInfo_l text:nth-child(2){font-size: 24rpx;color: #afc9fa;}
.totalInfo .totalInfo_r{width: 500rpx; height: auto; }
.totalInfo .totalInfo_r .totalInfo_r_ul:nth-child(1){margin-bottom: 30rpx;}
.totalInfo .totalInfo_r .totalInfo_r_ul .totalInfo_r_ul_li{}
.totalInfo .totalInfo_r .totalInfo_r_ul .totalInfo_r_ul_li text{text-align: center; font-weight: bold; display: block;font-family:PingFangSC-Semibold,PingFang SC;}
.totalInfo .totalInfo_r .totalInfo_r_ul .totalInfo_r_ul_li text:nth-child(1){font-size: 32rpx; color: #ffffff; padding-bottom: 10rpx;}
.totalInfo .totalInfo_r .totalInfo_r_ul .totalInfo_r_ul_li text:nth-child(2){font-size: 20rpx;color: #afc9fa;}

.mapServer{width: 100%; height: 448rpx; position: relative; }
.mapServer .mapServer_img{display: block; max-width: 100%; width: 478rpx; height: 448rpx; margin: 10rpx 20rpx 0 200rpx;}
.mapServer .mapServer_title{display: block; position: absolute; top: 20rpx; left: 30rpx; width: 300rpx; height: 100rpx; color: #ffffff; text-align: center; font-size: 44rpx;font-family:PingFangSC-Semibold,PingFang SC;}
.mapServer .mapServer_express{position: absolute; bottom: 10rpx;left: 60rpx; width: 50rpx; height: 100rpx;}
.mapServer .mapServer_express .mapServer_express_text{width: 20rpx; height: 100rpx; float: left; margin-right: 10rpx;font-family:PingFangSC-Semibold,PingFang SC;}
.mapServer .mapServer_express .mapServer_express_text text{ font-size: 20rpx; color: #ffffff; text-align: center; display: block; height: 50rpx; width: 20rpx;}
.mapServer .mapServer_express .mapServer_express_text text:nth-child(2){line-height: 70rpx;}
.mapServer .mapServer_express .mapServer_express_pillar{background: linear-gradient(top , rgb(107, 197, 253) , rgb(37, 83, 115) 100%);
background: -o-linear-gradient(top , rgb(107, 197, 253) , rgb(37, 83, 115) 100%);
background: -ms-linear-gradient(top , rgb(107, 197, 253) , rgb(37, 83, 115) 100%);
background: -moz-linear-gradient(top , rgb(107, 197, 253) , rgb(37, 83, 115) 100%);
background: -webkit-linear-gradient(top , rgb(107, 197, 253) , rgb(37, 83, 115) 100%); width: 20rpx; height: 100rpx;float: left;}

.chart1{ height: auto; padding: 0 20rpx 0 40rpx; }
.chart1 .chart1_ai{width: 335rpx; height: 180rpx; float: left;}
.chart1 .chart1_ai .chart1_ai_title{color: #ffffff;text-align: left; font-size: 44rpx; height: 80rpx;font-family:PingFangSC-Semibold,PingFang SC;}
.chart1 .chart1_ai .chart1_ai_chart{ height: 100rpx;}
.chart1 .chart1_ai .chart1_ai_chart .chart1_ai_num{display: block;width:230rpx;font-size: 52rpx; color: #5d91ff; font-weight: bold;float: left; margin-top: 10rpx;font-family:PingFangSC-Semibold,PingFang SC;}
.chart1 .chart1_ai .chart1_ai_chart .chart1_ai_num text{font-size: 24rpx;color: #5ba4d1; padding-left: 10rpx; font-family:PingFangSC-Semibold,PingFang SC;   }
.chart1 .chart1_ai .chart1_ai_chart .chart1_ai_pillar{display: block;float: left; width: 70rpx; height: 100rpx;}
.chart1 .chart1_ai .chart1_ai_chart .chart1_ai_pillar .chart1_ai_pillar_img{display: block; width: 70rpx; height: 96rpx;}
.chart1 .chart1_member{width: auto; height: auto;  }
.chart1 .chart1_member .chart1_member_title{color: #ffffff; height: 60rpx;}
.chart1 .chart1_member .chart1_member_title text{display: block;font-family:PingFangSC-Semibold,PingFang SC;}
.chart1 .chart1_member .chart1_member_title text:nth-child(1){float: left;font-weight:600;font-size:28rpx;}
.chart1 .chart1_member .chart1_member_title text:nth-child(2){float: right;font-size:20rpx;font-weight:400;color:rgba(116,138,161,1);}
.chart1 .chart1_member .container {display: flex;flex-direction: column;justify-content: space-between;box-sizing: border-box; width: auto; height: 250rpx;}
.chart1 .chart1_member .container  ec-canvas {width: auto; height: 250rpx;}

/*圆环进度条文字*/
.circle_info{
  position: absolute; 
  width: 100%;
  left: 50%;
  top: 50%; 
  transform: translate(-50%,-50%); 
  display: flex;  
  align-items: center;
  justify-content: center
}
.circle_info2{
  position: absolute; 
  width: 100%;
  left: 50%;
  boottom: 0; 
  transform: translate(-50%,-50%); 
  display: block;  
  align-items: center;
  justify-content: center
}
.circle_dot{
  
  width:16rpx;
  height: 16rpx;  
  border-radius: 50%;
  background-color: #fb9126;
} 
.circle_txt{
  padding-left: -10rpx;
  color: #fff;
  font-size: 28rpx; 
  letter-spacing: 2rpx;
  font-weight:900;
  font-family:PingFangSC-Semibold,PingFang SC;
}
.circle_title{
  margin-top: 50rpx;
  display: block;
  color: #fff;
  font-size: 28rpx; 
  font-weight:400;
  text-align: center;
  font-family:PingFangSC-Semibold,PingFang SC;
}
.container2{margin: auto; height: 240rpx;}
.container{margin: auto 25rpx; height: 240rpx; display: flex;}
.container .container_li{ width: 180rpx; height: 230rpx; flex: 1;}
.container .container_li .container_li_title{font-size: 32rpx; text-align: center;  color: #fff;display: block;font-weight:600; width: 100px; margin: auto;}
