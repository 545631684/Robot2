.pages{  background: linear-gradient(top , rgb(27, 56, 76) , rgb(23, 41, 58) 50% , rgb(27, 35, 49) 100%);
background: -o-linear-gradient(top , rgb(27, 56, 76) , rgb(23, 41, 58) 50% , rgb(27, 35, 49) 100%);
background: -ms-linear-gradient(top , rgb(27, 56, 76) , rgb(23, 41, 58) 50% , rgb(27, 35, 49) 100%);
background: -moz-linear-gradient(top , rgb(27, 56, 76) , rgb(23, 41, 58) 50% , rgb(27, 35, 49) 100%);
background: -webkit-linear-gradient(top , rgb(27, 56, 76) , rgb(23, 41, 58) 50% , rgb(27, 35, 49) 100%);padding-top: 40rpx;
height: auto; min-height: 100vh; padding-bottom: 150rpx;}
.ai_tab{display: block; width: 670rpx;height:72rpx; margin: auto; padding: 0 ;}
.ai_tab text{display: block; float: left;  width:329rpx;height:72rpx;background:rgba(8,31,47,1);border:2rpx solid #386a8a;font-size:28rpx;
font-family:PingFangSC-Regular,PingFang SC;font-weight:400;color:#aec8f9; text-align: center; line-height: 72rpx;}
.ai_tab text:nth-child(1){border-radius:8rpx 0rpx 0rpx 8rpx;}
.ai_tab text:nth-child(2){border-radius:0rpx 8rpx 8rpx 0rpx;}
.ai_tab text:nth-child(1).on{background:rgba(41,91,130,1);border:2rpx solid rgba(91,164,209,1); color: #ffffff;}
.ai_tab text:nth-child(2).on{background:rgba(41,91,130,1);border:2rpx solid rgba(91,164,209,1);color: #ffffff;}

/*圆环进度条文字*/
.circle_info{position:absolute;width:100%;left:50%;top:50%;transform:translate(-50%,-50%);display:flex;align-items:center;justify-content:center}
.circle_info2{position:absolute;width:100%;left:50%;boottom:0;transform:translate(-50%,-50%);display:block;align-items:center;justify-content:center}
.circle_dot{width:16rpx;height:16rpx;border-radius:50%;background-color:#fb9126}
.circle_txt{color:#fff;font-size:100rpx;letter-spacing:2rpx;font-weight:900;font-family:PingFangSC-Semibold,PingFang SC}
.circle_title{margin-top:50rpx;display:block;color:#fff;font-size:28rpx;text-align:center;font-family:DIN-Black,DIN;font-weight:900}
.container2{margin: auto; height: 240rpx;}
.container{margin: auto 25rpx; height: 240rpx;}
.container .container_li{display: block; width: 458rpx; height: 458rpx; margin: auto 26rpx; float: left;}
.container .container_li .container_li_title{font-size: 32rpx; text-align: center;  color: #fff;display: block;font-weight:600; width: 100px; margin: auto;}
.ai_state{width: 458rpx; height: 458rpx; margin: auto auto 40rpx;}
.ai_con1,.ai_con2{width: 670rpx;height:auto; margin: auto;}
.ai_tiem{display: block; width: 566rpx;height:auto; margin: auto;}
.ai_tiem view{width: 122rpx; height: 176rpx; display: block; float: left; margin-right: 100rpx;margin-bottom: 30rpx;}
.ai_tiem view:nth-child(3n+3){margin-right: 0;}
.ai_tiem view.on image{display: block;width: 122rpx; height: 122rpx;background:rgba(8,31,47,0);box-shadow:0px 0px 10rpx 0px rgba(98,255,213,0.5);border-radius:61rpx;border:2rpx solid rgba(57,225,181,1);}
.ai_tiem view image{display: block;width:122rpx;height:122rpx;background:rgba(8,31,47,0);border-radius:61rpx;border:2rpx solid rgba(41,84,116,1); margin-bottom: 20rpx;}
.ai_tiem view text{display: block; height: 25rpx; line-height: 25rpx; font-size: 24rpx; font-weight: 400; color: #ffffff; text-align: center;}

.ai_keywordList{width: 670rpx;height:auto; padding: 0 ; margin: auto; display: flex; flex-wrap: wrap; box-sizing: border-box;}
.ai_keywordList .ai_keyword_div{height:82rpx; padding-right: 30rpx;}
.ai_keywordList .ai_keyword{ width:316rpx;height:62rpx;background:rgba(8,31,47,0);border-radius:12rpx;border:2rpx solid rgba(41,84,116,1);}
.ai_keywordList .ai_keyword_div:nth-child(2n+2){padding-right: 0;}
.ai_keywordList .ai_keyword:last-of-type{}
/* .ai_keywordList .ai_keyword:nth-child(9),.ai_keywordList .ai_keyword:nth-child(10){margin-bottom: 0;} */
.ai_keywordList .ai_keyword view{height:62rpx;font-size:28rpx;font-family:PingFangSC-Regular,PingFang SC;font-weight:400;color:rgba(255,255,255,1);line-height:62rpx;display: block; float: left;}
.ai_keywordList .ai_keyword view:nth-child(1){padding:0 20rpx;}
.ai_keywordList .ai_keyword view:nth-child(2){width:44px; line-height: initial;display:-webkit-flex;display:flex;justify-content:center;align-items:center; float: right;}
.ai_keywordList .ai_keyword view:nth-child(2) text{width: 8rpx; height: 20rpx; display: block; float: left; margin-right: 4rpx; background: #44d7b6;}
.ai_keywordList .ai_keyword view:nth-child(2) text:last-of-type{margin-right: 0;}

