.pages{  background: linear-gradient(top , rgb(27, 56, 76) , rgb(23, 41, 58) 50% , rgb(27, 35, 49) 100%);
background: -o-linear-gradient(top , rgb(27, 56, 76) , rgb(23, 41, 58) 50% , rgb(27, 35, 49) 100%);
background: -ms-linear-gradient(top , rgb(27, 56, 76) , rgb(23, 41, 58) 50% , rgb(27, 35, 49) 100%);
background: -moz-linear-gradient(top , rgb(27, 56, 76) , rgb(23, 41, 58) 50% , rgb(27, 35, 49) 100%);
background: -webkit-linear-gradient(top , rgb(27, 56, 76) , rgb(23, 41, 58) 50% , rgb(27, 35, 49) 100%);  height:auto;  min-height: 100vh; padding-bottom: 150rpx;}

.dingyue{width: 670rpx; height: auto; margin: auto;}
.dingyue .dingyue_title{padding: 50rpx 0 25rpx;}
.dingyue .dingyue_title text{display: block; text-align: center; color: #ffffff;font-family:PingFangSC;}
.dingyue .dingyue_title text:nth-child(1){font-size: 32rpx; padding-bottom: 30rpx;font-weight:500;}
.dingyue .dingyue_title text:nth-child(2){ font-size: 24rpx;font-weight:400;}
.dingyue .dingyueList{width: 670rpx; height: auto; }
.dingyue .dingyueList .dingyueList_li{width: 100%; box-sizing: border-box; height: 73rpx; line-height: 73rpx; display:flex;justify-content: space-between; align-items: center;background:rgba(26,62,90,1);border:1rpx solid rgba(91,164,209,1);border-radius:8rpx;padding: 20rpx; margin-bottom: 20rpx;}
.dingyue .dingyueList .dingyueList_li:last-of-type{margin-bottom: 0;}
.dingyue .dingyueList .dingyueList_li .dingyueList_li_l{ line-height: 73rpx;font-size:28rpx;font-family:PingFangSC;
font-weight:500;color:rgba(255,255,255,1); flex: 1;}
.dingyue .dingyueList .dingyueList_li .dingyueList_li_r{display:block;align-items:center;flex: 1;}
.dingyue .dingyueList .dingyueList_li .dingyueList_li_r view{display: block; float: left; width: 28rpx; height: 28rpx;background:rgba(26,62,90,1);border-radius:4rpx;border:1px solid rgba(91,164,209,1); margin-right: 20rpx; float: right;}
.dingyue .dingyueList .dingyueList_li .dingyueList_li_r image{float: left; width: 32rpx; height: 32rpx;margin-right: 20rpx;float: right; }
.confirmButton{width:670rpx;height:88rpx;background:rgba(68,178,237,1);border-radius:10rpx; margin:30rpx auto auto;color: #ffffff;font-size:32rpx;font-family:PingFangSC;font-weight:500; line-height: 88rpx; text-align: center;}