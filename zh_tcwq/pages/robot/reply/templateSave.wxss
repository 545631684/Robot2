.keyBody{width: 700rpx; height: auto; margin: 30rpx auto;}
.keyBody .keyBody_title{text-align: center; font-weight: bold; margin: 50rpx auto;}
.keyBody .keyBody_add{width: 400rpx; height: 80rpx; text-align: center; margin: auto; color: #fff; background: #ed414a; line-height: 80rpx; font-weight: bold; border-radius: 20rpx; margin-bottom: 150rpx;}
.keyBody .keyList{width: 700rpx; height: auto; margin-bottom: 50rpx;}
.keyBody .keyList .keyList_li{width: 700rpx; height: auto;background: #fff; border-radius: 10rpx; padding: 20rpx 40rpx;box-shadow: 0px 0px 10px 0px #d5d5d5; box-sizing: border-box; margin-bottom: 50rpx;}
.keyBody .keyList .keyList_li .keyList_li_con1{display: flex; justify-content: space-between; align-items: center; padding-bottom: 30rpx; border-bottom: 1px dashed #ccc; margin-bottom: 30rpx;}
.keyBody .keyList .keyList_li .keyList_li_con1 text{}
.keyBody .keyList .keyList_li .keyList_li_con2{display: flex; justify-content: flex-start; align-items: center;}
.keyBody .keyList .keyList_li .keyList_li_con2 text{width: 150rpx; text-align: center;}
.keyBody .keyList .keyList_li .keyList_li_con2 text:nth-child(2){width: auto;}
.keyBody .keyList .keyList_li .keyList_li_con3{display: flex; justify-content: flex-start; align-items: center;}
.keyBody .keyList .keyList_li .keyList_li_con3 text{width: 150rpx;text-align: center;}
.keyBody .keyList .keyList_li .keyList_li_con3 image{width: 100rpx; height: 100rpx;}

.keyTC{position: absolute;top: 0;left: 0;background: url(https://wechat.ql888.net/attachment/Robot/images/eject.png) 0rpx 0rpx / 50rpx 50rpx repeat;width: 750rpx;height: 100vh;display: inline-flex;justify-content: center;align-items: center; z-index: 2000;}
.keyTC form{width: 700rpx; height: auto;border-radius: 10rpx; background: #fff; padding: 30rpx; box-sizing: border-box;}
.keyTC .keyTC_title{ text-align: center; padding: 20rpx 0 50rpx; font-weight: bold;}
.keyTC .keyTC_li{justify-content: flex-start;align-items: center;display: inline-flex; width: 640rpx; margin: 20rpx auto;}
.keyTC .keyTC_li radio{margin-right: 30rpx; zoom: 0.8;}
.keyTC .keyTC_li text{width: 200rpx; text-align: justify;}
.keyTC .keyTC_li input{border-bottom: 1px solid #ccc;}
.keyTC .keyTC_li textarea{border: 1px solid #ccc; border-radius: 10rpx; width: 100%; height: 200rpx; padding: 20rpx;}
.keyTC .keyTC_li radio-group{}
.keyTC .keyTC_an{display: flex; justify-content: center; align-items: center; margin-top: 60rpx;}
.keyTC .keyTC_an text{border-radius: 10rpx; background: #ed414a; color: #fff; text-align: center; padding: 20rpx 80rpx; margin: 0 30rpx; font-weight: bold;}
