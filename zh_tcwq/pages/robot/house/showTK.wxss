}

.withdraw_row {
    background: #fff;
    padding: 20rpx 30rpx;
    height: 100rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
    margin-bottom: 20rpx;
}

.withdraw_row_l {
    line-height: 50rpx;
}

.proceedRecord_row {
    width: 690rpx;
    height: 100rpx;
    margin: 0 auto;
    display: flex;
    align-items: center;
    border-bottom: 1rpx solid #888;
}

.picker_date {
    display: flex;
    justify-content: space-between;
}

.picker_date picker {
    min-width: 200rpx;
    text-align: center;
    color: #777;
}

.typeList {
    display: flex;
    align-items: center;
    height: 100%;
}

.typeList picker {
    height: 100%;
    min-width: 400rpx;
    color: #777;
}

.picker1 {
    height: 100rpx;
    line-height: 100rpx;
    padding-left: 20rpx;
}

.top {
    display: flex;
    align-items: center;
    background-color: #fff;
    color: #333;
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    border-bottom: 1px solid #eee;
}

.top .box {
    display: flex;
    align-items: center;
    font-size: 28rpx;
    margin-left: auto;
    padding: 20rpx;
}

.nav {
    color: #4CADF2;
    font-weight: bold;
}

.top .box image {
    width: 50rpx;
    height: 50rpx;
}

.dialog_content {
    padding: 0 40rpx 40rpx;
}

.dialog_title {
    padding: 20rpx 30rpx;
    text-align: center;
    font-size: 32rpx;
    display: flex;
    align-items: center;
}

.dialog_title .close,.title {
    margin-left: auto;
}

.dialog_title .close {
    color: #3296fa;
}

.diaog_box {
    padding: 30rpx 10rpx 30rpx 30rpx;
}

.diaog_box text {
    padding: 10rpx 30rpx;
    border: 1rpx solid #ccc;
    margin: 20rpx 20rpx 0 0;
    border-radius: 10rpx;
}

.item_list {
    display: flex;
    flex-wrap: wrap;
    margin-left:61rpx;
}

.diaog_box .title {
    margin-left: 0;
}

.active_list {
    border: 1rpx solid #3296fa!important;
    color: #3296fa;
}

.dialogBox {
    background-color: #fff;
}

.dialog {
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    z-index: 999;
    background-color: rgba(0,0,0,0.4);
}

.dialog_footer>text {
    display: inline-block;
    padding: 26rpx 0;
    border: 1px solid #eee;
    border-left-color: transparent;
    width: 50%;
    text-align: center;
    font-size: 28rpx;
}

.dialog_ok {
    color: #1989fa!important;
}

.dialog_footer {
    text-align: right;
    display: flex;
    position: relative;
    z-index: 999;
}

@-webkit-keyframes dialog_box {
    from {
        transform: translateY(-40rpx);
        opacity: 0;
    }

    to {
        transform: translateY(0);
        opacity: 1;
    }
}

@keyframes dialog_box {
    from {
        transform: translateY(-40rpx);
        opacity: 0;
    }

    to {
        transform: translateY(0);
        opacity: 1;
    }
}
