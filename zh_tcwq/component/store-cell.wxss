@import "../../app.wxss";

@import "../pages/index/index.wxss";

.store-cell {
    padding: 0 30rpx 30rpx;
    background: #fff;
    font-size: 30rpx;
}

.flex-raw {
    display: flex;
    flex-direction: row;
    flex-wrap: wrap;
}

.flex-raw .img {
    width: 150rpx;
    height: 120rpx;
    margin: 20rpx 10rpx 0 10rpx;
}

.gobyndmulit {
    display: -webkit-box;
    overflow: hidden;
    text-overflow: ellipsis;
    word-break: break-all;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 6;
}